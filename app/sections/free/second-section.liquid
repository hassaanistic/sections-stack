{% comment %}
  Second Section
  Category: text
  Description: Descroption
  Created: 2025-07-06T08:44:50.367Z
{% endcomment %}

<div class="second-section-section">
  <div class="container">
    <h2>{{ section.settings.heading | default: 'Second Section' }}</h2>
    {% if section.settings.description != blank %}
      <p>{{ section.settings.description }}</p>
    {% endif %}
    
    <!-- Add your section content here -->
    
  </div>
</div>

<style>
  .second-section-section {
    padding: 60px 0;
  }
  
  .second-section-section .container {
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 20px;
  }
  
  /* Add your custom styles here */
</style>

{% schema %}
{
  "name": "Second Section",
  "settings": [
    {
      "type": "text",
      "id": "heading",
      "label": "Heading",
      "default": "Second Section"
    },
    {
      "type": "textarea",
      "id": "description",
      "label": "Description"
    }
  ],
  "presets": [
    {
      "name": "Second Section"
    }
  ]
}
{% endschema %}