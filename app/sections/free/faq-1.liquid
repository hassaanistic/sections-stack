{% endcomment %}

<div class="first-section-section">
  <div class="container">
    <h2>{{ section.settings.heading | default: 'First Section' }}</h2>
    {% if section.settings.description != blank %}
      <p>{{ section.settings.description }}</p>
    {% endif %}
    
    <!-- Add your section content here -->
    
  </div>
</div>

<style>
  .first-section-section {
    padding: 60px 0;
  }
  
  .first-section-section .container {
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 20px;
  }
  
  /* Add your custom styles here */
</style>

{% schema %}
{
  "name": "First Section",
  "settings": [
    {
      "type": "text",
      "id": "heading",
      "label": "Heading",
      "default": "First Section"
    },
    {
      "type": "textarea",
      "id": "description",
      "label": "Description"
    }
  ],
  "presets": [
    {
      "name": "First Section"
    }
  ]
}
{% endschema %}